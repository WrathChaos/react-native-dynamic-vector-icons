{
  "name": "react-native-dynamic-vector-icons",
  "version": "1.1.3",
  "description": "Wrapper of react-native-vector-icons to use dynamic types.",
  "keywords": [
    "typescript",
    "vector-icons",
    "react-native",
    "icons",
    "dynamic-vector-icons",
    "vector",
    "icons",
    "vector-icons",
    "javascript",
    "ui-lib",
    "rn"
  ],
  "homepage": "https://www.freakycoder.com",
  "bugs": "https://github.com/WrathChaos/react-native-dynamic-vector-icons/issues",
  "main": "dist/Icon.js",
  "repository": "git@github.com:WrathChaos/react-native-dynamic-vector-icons.git",
  "author": "Kuray (FreakyCoder) <kurayogun@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p . && echo Build completed!",
    "watch": "tsc -p . --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "react": ">= 16.x.x",
    "react-native": ">= 0.55.x",
    "react-native-vector-icons": ">= 6.x.x"
  },
  "devDependencies": {
    "@types/react": "^16.9.22",
    "@types/jest": "^26.0.13",
    "@types/react-native": "^0.63.0",
    "typescript": "^4.0.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.js --fix-dry-run"
    ]
  }
}
